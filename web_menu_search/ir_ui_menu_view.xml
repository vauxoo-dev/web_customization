<?xml version="1.0"?>
<openerp>
    <data>
        <record id="view_menu_form_inherit" model="ir.ui.view">
            <field name="name">menu.form.inherit</field>
            <field name="model">ir.ui.menu</field>
            <field name="inherit_id" ref="base.edit_menu_access"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="after">
                    <field name="menu_code"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
